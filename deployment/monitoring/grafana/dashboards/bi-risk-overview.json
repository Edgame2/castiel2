      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "targets": [
        { "expr": "sum(increase(risk_evaluations_total[24h])) or vector(0)", "legendFormat": "Risk evaluations", "refId": "A" }
