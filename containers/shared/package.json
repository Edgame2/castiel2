{
  "name": "@coder/shared",
  "version": "1.0.0",
  "description": "Shared utilities, types, and services for Castiel containers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
    "./types/shards": { "types": "./dist/types/shards/index.d.ts", "default": "./dist/types/shards/index.js" },
    "./database": { "types": "./dist/database/index.d.ts", "default": "./dist/database/index.js" },
    "./utils/errors": { "types": "./dist/utils/errors.d.ts", "default": "./dist/utils/errors.js" },
    "./utils/logger": { "types": "./dist/utils/logger.d.ts", "default": "./dist/utils/logger.js" },
    "./cache": { "types": "./dist/cache/index.d.ts", "default": "./dist/cache/index.js" },
    "./services": { "types": "./dist/services/index.d.ts", "default": "./dist/services/index.js" }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@fastify/jwt": "^9.0.0",
    "amqplib": "^0.10.5",
    "axios": "^1.7.9",
    "fastify": "^5.7.4",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^25.0.5",
    "@types/ioredis": "^5.0.0",
    "typescript": "^5.9.3"
  }
}

